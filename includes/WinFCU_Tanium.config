<?xml version="1.0" encoding="utf-8" ?>
<!-- Cleanup directives for Tanium Client folders -->
<configuration version="7.0">
  <applicationSettings>
    <Folders schedule="Daily" deleteEmptyFolders="false" recursive="false" >
      <Folder path="C:\Program Files (x86)\Tanium\Tanium Client\Downloads\logs" >
        <File name="*.progress" age="5" action="Archive" target="#FSDIR#_#FCMONTH#.zip" />
        <File name="*.zip" age="180" action="Delete" />
      </Folder>
      <Folder path="C:\Program Files (x86)\Tanium\Tanium Client\Downloads" >
        <File name="*.log" age="5" action="Archive" target="#FSDIR#_#FCMONTH#.zip" />
        <File name="*.zip" age="180" action="Delete" />
      </Folder>
    </Folders>
  </applicationSettings>
</configuration>
